#TEMPLATE
kent circuit

#PARAMS
model,jj1
level,1
icrit_mag,1.0e-6
i1_phase,0
i1_mag,1e-9
i1_freq,30e6
idc_mag,1e-7
l0_mag,3e-12
k1_mag,1
l1_mag,3e-12
ics1_mag,1.1e-6
phi1_mag,0
l2_mag,1e-10
tran_step,1e-11
tran_stop,6e-7
tran_start,1e-7
maxdata,1024000
filename,Kent

cpic_mag,0.7e-9
vm_mag,16.5e-3
l3_mag,0
i2_phase,0
i2_mag,2e-5
i2_freq,3e12
k2_mag,-1
r1_mag,10
c1_mag,1e-8

#MEASURABLES
@b1[cap] i(l0) i(l1) i(l2) v(0)-v(3) v(1)-v(0) v(2)-v(0) v(3)-v(2) v(101)

#CIRCUIT
Kent circuit
.model {model} jj(level={level}, icrit={icrit_mag}, cpic={cpic_mag}, vm={vm_mag})
i1 0 1 ac sin({i1_phase} {i1_mag} {i1_freq})
idc 0 1 {idc_mag}
l0 1 0 {l0_mag}
k1 l0 l1 {k1_mag}
l1 0 2 {l1_mag}
b1 2 3 101 {model} ics={ics1_mag} phi={phi1_mag}
l2 3 0 {l2_mag}
.tran {tran_step} {tran_stop} {tran_start} uic 
.control
devload all
set maxdata={maxdata}
run
write {filename}.txt {measurables}
.endc

#VARIATIONS
var;no_source
remove;i1
remove;idc
add;l3 1 0 {l3_mag}
end
var;v_bias_with_dc_bias
remove;i1
add;v1 0 1 ac sin({i1_phase} {i1_mag} {i1_freq})
add;l3 0 4 {l3_mag}
change;idc 4 0 {idc_mag}
add;k2 l2 l3 {k2_mag}
addm;i(l3)
end
var;v_bias_with_ac_bias
remove;i1
add;v1 0 1 ac sin({i1_phase} {i1_mag} {i1_freq})
add;l3 0 4 {l3_mag}
change;idc 4 0 ac sin({i2_phase} {i2_mag} {i2_freq})
add;k2 l2 l3 {k2_mag}
addm;i(l3)
end
var;v_bias
remove;i1
remove;idc
add;v1 0 1 ac sin({i1_phase} {i1_mag} {i1_freq})
end
var;parallel_c
add;c1 2 4 {c1_mag}
add;l3 4 3 {l3_mag}
addm;i(l3)
end
var;parallel_r
add;r1 2 4 {r1_mag}
add;l3 4 3 {l3_mag}
addm;i(l3)
end
var;parallel_rc
add;c1 2 3 {c1_mag}
add;r1 2 4 {r1_mag}
add;l3 4 3 {l3_mag}
addm;i(l3)
end
var;biased_jj
add;l3 0 4 {l3_mag}
add;k2 l2 l3 {k2_mag}
add;idc2 0 4 {idc2_mag}
addm;i(l3)
var;two_jjs
add;b2 3 4 102 {model} ics={ics2_mag} phi={phi2_mag}
change;l2 4 0 {l2_mag}
end

#NOTES
This is the circuit that Kent suggested as a possible method of Bode-Fano evasion.
Changed to have one JJ as the default, with the previous template in kent_template_old.
